{
	"name": "ogioncz/fan-club-penguin.cz",
	"homepage": "http://fan-club-penguin.cz",
	"type": "project",
	"license": ["BSD-3-Clause"],
	"authors": [
		{
			"name": "Jan Tojnar",
			"homepage": "http://ogion.cz"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/flaptor/indextank-php"
		}
	],
	"require": {
		"php": ">= 8.3",
		"actinarium/php-diff": "dev-master",
		"alb/oembed": "dev-master",
		"contributte/forms-multiplier": "^4.0",
		"ezyang/htmlpurifier": "dev-master",
		"flaptor/indextank-php": "dev-master",
		"latte/latte": "^3.0",
		"league/commonmark": "^1.0.0",
		"longman/telegram-bot": "^0.83.1",
		"nette/application": "^3.2",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.3",
		"nette/di": "^3.2",
		"nette/finder": "^3.0",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"nextras/form-components": "^2.0",
		"nextras/forms-rendering": "^2.0",
		"nextras/orm": "^5.0",
		"tracy/tracy": "^2.9"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.66",
		"mockery/mockery": "^1.5",
		"nette/tester": "^2.4",
		"nextras/migrations": "^3.0",
		"nextras/orm-phpstan": "^2.0",
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-mockery": "^2.0",
		"phpstan/phpstan-nette": "^2.0",
		"rector/rector": "^2.0",
		"symfony/console": "^7.2"
	},
	"autoload": {
		"classmap": ["vendor/others"],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"scripts": {
		"cs": "php-cs-fixer fix --verbose --dry-run --diff",
		"fix": "php-cs-fixer fix --verbose --diff",
		"rector": "vendor/bin/rector",
		"phpstan": "phpstan analyse --memory-limit 512M",
		"test": "php vendor/bin/tester -C tests/unit"
	}
}
